{
  "name": "ocd-coder-theme",
  "version": "0.1.0",
  "description": "Wordpress Theme using React written for theocdcoder.com",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --config webpack.config.production.js",
    "watch": "webpack --watch --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KevinDahlberg/wp-theocdcoder"
  },
  "keywords": [
    "Wordpress",
    "React",
    "React-Router",
    "Redux"
  ],
  "author": "Kevin Dahlberg",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^4.0.0-beta.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^21.2.1",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-lite": "^0.15.39",
    "react-redux": "^5.0.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1"
  },
  "devDependencies": {},
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": [
        0
      ]
    }
  }
}
